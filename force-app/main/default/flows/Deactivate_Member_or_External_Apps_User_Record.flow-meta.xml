                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Lookup_External_Apps_User_Using_Contact</targetReference>
            </connector>
            <label>Is Vendor or Manager</label>
        </rules>
    </decisions>
    <decisions>
        <name>Portal_User_Found</name>
