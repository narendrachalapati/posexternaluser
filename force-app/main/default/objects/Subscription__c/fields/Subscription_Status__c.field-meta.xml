<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Subscription_Status__c</fullName>
    <externalId>false</externalId>
    <formula>IMAGE(
CASE(Status__c,
&quot;active&quot;, &quot;/img/samples/flag_green.gif&quot;,
&quot;unpaid&quot;, &quot;/img/samples/flag_red.gif&quot;,
&quot;canceled&quot;, &quot;/img/samples/flag_red.gif&quot;,
&quot;incomplete&quot;, &quot;/img/samples/flag_red.gif&quot;,
&quot;incomplete_expired&quot;, &quot;/img/samples/flag_red.gif&quot;,
&quot;past_due&quot;, &quot;/img/samples/flag_red.gif&quot;,
&quot;/img/samples/flag_yellow.gif&quot;),
&quot;Subscription Status&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Subscription Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
